<div>

    <table>
        <thead>
        <th>Index</th>
        <th>User Id</th>
        <th>Firts Name</th>
        <th>Last Name</th>
        <th>Sex</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Date Of Birth</th>
        <th>Job Title</th>
        </thead>
        <tbody>
               
            <tr>

                @{
                    while (Model.Count != 0)
                    {
                        
                    var person = Model.removeFim();
                
                    <td>@person.UserId</td>
                    <td>@person.FirstName</td>
                    <td>@person.LastName</td>
                    <td>@person.Sex</td>
                    <td>@person.Email</td>
                    <td>@person.Phone</td>
                    <td>@person.DateOfBirth.ToString("yyyy-MM-dd")</td>
                    <td>@person.JobTitle</td>
                    <hr/>
                    }


                }
             
            </tr>
            }
        </tbody>
    </table>
</div>